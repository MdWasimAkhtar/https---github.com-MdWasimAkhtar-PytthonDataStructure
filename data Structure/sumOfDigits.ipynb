    "def getSum(n):\n",
    "    sum = 0 \n",
    "    while(n != 0):\n",
    "        rem = n %10\n",
    "        sum = sum + rem \n",
    "        n = n //10\n",
    "    return sum\n",
    "\n",
    "getSum(22)\n"
   ]
